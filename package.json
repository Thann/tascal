{
  "name": "tascal",
  "version": "0.0.1",
  "description": "A simple time and task management system.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Thann/tascal.git"
  },
  "bin": "server.js",
  "license": "AGPL-3.0+",
  "scripts": {
    "start": "node server --build",
    "test": "mocha",
    "lint": "eslint .",
    "build": "docker build -t thann/tascal .",
    "dkstart": "docker-compose up -d",
    "dkstop": "docker-compose down",
    "dktest": "docker-compose exec server npm run -s test",
    "dklint": "docker-compose exec server npm run -s lint",
    "dknode": "docker-compose exec server node -i -e \"const db = require('./lib/db'); setTimeout(() => {console.log('const db = Database.open()')})\""
  },
  "dependencies": {
    "@fullcalendar/core": "^4.0.2",
    "@fullcalendar/daygrid": "^4.0.1",
    "@fullcalendar/interaction": "^4.0.2",
    "@fullcalendar/list": "^4.0.1",
    "@fullcalendar/rrule": "^4.0.2",
    "@fullcalendar/timegrid": "^4.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.6.0",
    "backbone": "^1.4.0",
    "backbone-subviews": "github:thann/backbone.subviews",
    "backbone.localstorage": "^1.1.16",
    "body-parser": "^1.18.3",
    "bootstrap": "^3.4.1",
    "compression-webpack-plugin": "^1.1.12",
    "cookie": "^0.3.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "express": "^4.16.4",
    "express-ws": "^4.0.0",
    "font-awesome": "^4.5.0",
    "hanson": "^1.1.1",
    "hson-loader": "^2.0.0",
    "imports-loader": "^0.8.0",
    "jquery": "^3.3.1",
    "luxon": "^1.12.1",
    "node-getopt": "^0.3.2",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "querystring": "^0.2.0",
    "sass-loader": "^7.1.0",
    "sqlite": "^3.0.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.12.0",
    "ws": "^5.2.2"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "supertest": "github:thann/supertest"
  },
  "eslintIgnore": [
    "**/*.js",
    "!api/*.js",
    "!lib/*.js",
    "!test/*.js",
    "!webapp/**/*.js",
    "!server.js",
    "!door.js",
    "!webpack.config.js"
  ]
}
